algoritmo "semnome"
// Função :
// Autor :
// Data : 23/09/2023
// Seção de Declarações 
var
  contagem, total, C_Inicio: inteiro
inicio
// Seção de Comandos 

   contagem := 0
   C_Inicio <- 1
   
   repita
   
        total <- C_Inicio + contagem
        escreva(contagem)

        contagem <- contagem + 1
   ate (contagem > 15)
fimalgoritmo
